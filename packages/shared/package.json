{
  "name": "@kernl-sdk/shared",
  "version": "0.1.4",
  "description": "Shared utilities for kernl",
  "keywords": [
    "kernl",
    "utilities",
    "shared"
  ],
  "author": "dremnik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kernl-sdk/kernl.git",
    "directory": "packages/shared"
  },
  "homepage": "https://github.com/kernl-sdk/kernl#readme",
  "bugs": {
    "url": "https://github.com/kernl-sdk/kernl/issues"
  },
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "tsc-alias": "^1.8.10",
    "typescript": "5.9.2",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "zod": "^4.1.12"
  }
}
