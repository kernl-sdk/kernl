{
  "name": "@kernl-sdk/cli",
  "version": "0.4.0",
  "description": "CLI for managing kernl applications",
  "keywords": [
    "kernl",
    "cli",
    "ai",
    "agent",
    "framework",
    "scaffold"
  ],
  "author": "dremnik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kernl-sdk/kernl.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/kernl-sdk/kernl#readme",
  "bugs": {
    "url": "https://github.com/kernl-sdk/kernl/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./create": {
      "types": "./dist/create.d.ts",
      "default": "./dist/create.js"
    }
  },
  "bin": {
    "kernl": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build",
    "lint": "eslint src/",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@antfu/ni": "^28.0.0",
    "@clack/prompts": "^0.11.0",
    "commander": "^14.0.2",
    "fast-glob": "^3.3.3",
    "picocolors": "^1.1.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "5.9.2",
    "vitest": "^4.0.8"
  }
}
