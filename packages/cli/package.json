{
  "name": "@kernl-sdk/cli",
  "version": "0.2.7",
  "description": "CLI for managing kernl applications",
  "keywords": [
    "kernl",
    "cli",
    "ai",
    "agent",
    "framework",
    "scaffold"
  ],
  "author": "dremnik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kernl-sdk/kernl.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/kernl-sdk/kernl#readme",
  "bugs": {
    "url": "https://github.com/kernl-sdk/kernl/issues"
  },
  "bin": {
    "kernl": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && tsc-alias --resolve-full-paths -f",
    "prepublishOnly": "pnpm build",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@antfu/ni": "^28.0.0",
    "@googleapis/gmail": "^5.0.4",
    "commander": "^14.0.2",
    "fast-glob": "^3.3.3",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "tsc-alias": "^1.8.16",
    "typescript": "5.9.2",
    "vitest": "^4.0.8"
  }
}
